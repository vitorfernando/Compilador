def main():void {
	# Testa vetor
	int i, counter, alist[10];
	i = 10;
	counter = 0;

	alist = [20,30,40,90,50,60,70,80,100,110];

	while i <= 110 {
	  if alist[counter] == i{
	    print 'i is in the list: ', i, '\n';
	  }
	  else {
	    print 'i is not in the list: ', i, '\n';
	  }
	  i = i * 3 - 10;
	  counter = counter + 1;
	}
