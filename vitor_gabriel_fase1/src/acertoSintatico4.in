def xValue(int x):void {
	if x < 0 {
		x = 0;
		print 'Negative changed to zero.\n';
	}
	else {
		if x == 0 {
			print 'Zero found. Changing its value. ';
			x = 2 ^ 10;
			print x, '\n';
		}
		else{
			print 'Do nothing.\n';
		}
	}
	return ;
}

def main():void {
	xValue(-18);
	return ;
}

